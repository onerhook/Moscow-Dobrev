CREATE TABLE coffee (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    roast_level TEXT NOT NULL,
    ground_or_bean TEXT NOT NULL,
    flavor_description TEXT,
    price REAL NOT NULL,
    package_volume INTEGER NOT NULL
);

